	<queryString>
		<![CDATA[SELECT pedido.`fechaDeCreacion` AS pedido_fechaDeCreacion ,COUNT(*), pedido.`Usuario_id` AS pedido_Usuario_id, usuario.`titular` AS usuario_titular, usuario.`razonSocial` AS usuario_razonSocial, rol.`nombre` AS rol_nombre
FROM
     `usuario` usuario INNER JOIN `pedido` pedido ON usuario.`id` = pedido.`Usuario_id`
     INNER JOIN `rol` rol ON usuario.`Rol_idRol` = rol.`idRol`
WHERE   pedido.`fechaDeCreacion` between $P{fechaI} and $P{fechaF} GROUP BY  pedido.fechaDeCreacion]]>
	</queryString>
