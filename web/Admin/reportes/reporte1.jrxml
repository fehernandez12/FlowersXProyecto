	<queryString language="SQL">
		<![CDATA[SELECT
     permisos.`idpermisos` AS permisos_idpermisos,
     permisos.`nombre` AS permisos_nombre,
     usuario.`id` AS usuario_id,
     usuario.`titular` AS usuario_titular,
     rol.`nombre` AS rol_nombre
FROM
     `rol` rol INNER JOIN `usuario` usuario ON rol.`idRol` = usuario.`Rol_idRol`
     INNER JOIN `rol_has_permisos` rol_has_permisos ON rol.`idRol` = rol_has_permisos.`Rol_idRol`
     INNER JOIN `permisos` permisos ON rol_has_permisos.`permisos_idpermisos` = permisos.`idpermisos`]]>
