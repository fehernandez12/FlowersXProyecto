	<queryString>
		<![CDATA[SELECT
     novedad.`idNovedad` AS novedad_idNovedad,
     novedad.`descripcion` AS novedad_descripcion,
     novedad.`fecha` AS novedad_fecha,
     novedad.`Pedido_idPedido` AS novedad_Pedido_idPedido,
     novedad.`Usuario_id` AS novedad_Usuario_id,
     usuario.`razonSocial` AS usuario_razonSocial,
     usuario.`titular` AS usuario_titular,
     usuario.`email` AS usuario_email
FROM
     `usuario` usuario INNER JOIN `novedad` novedad ON usuario.`id` = novedad.`Usuario_id`
     INNER JOIN `pedido` pedido ON usuario.`id` = pedido.`Usuario_id`
     AND pedido.`idPedido` = novedad.`Pedido_idPedido`]]>
	</queryString>
