	<queryString>
		<![CDATA[SELECT
     producto.`idProducto` AS producto_idProducto,
     producto.`nombreProducto` AS producto_nombreProducto,COUNT(*),
     producto.`tiempoDeCultivo` AS producto_tiempoDeCultivo,
     producto.`existencias` AS producto_existencias,
     producto.`precio` AS producto_precio,
     producto.`descripcion` AS producto_descripcion,
     producto_has_pedido.`producto_idProducto` AS producto_has_pedido_producto_idProducto
FROM
     `producto` producto INNER JOIN `producto_has_pedido` producto_has_pedido ON producto.`idProducto` = producto_has_pedido.`producto_idProducto`
GROUP BY producto_has_pedido.`producto_idProducto`]]>
	</queryString>
