	<queryString>
		<![CDATA[SELECT
     usuario.`titular` AS usuario_titular,
     usuario.`razonSocial` AS usuario_razonSocial,
     usuario.`email` AS usuario_email,
     rol.`nombre` AS rol_nombre,
     rol.`name` AS rol_name,
     novedad.`descripcion` AS novedad_descripcion,
     novedad.`fecha` AS novedad_fecha
FROM
     `rol` rol INNER JOIN `usuario` usuario ON rol.`idRol` = usuario.`Rol_idRol`
     INNER JOIN `novedad` novedad ON usuario.`id` = novedad.`Usuario_id`
WHERE  novedad.`fecha`   between $P{fechaI} and $P{fechaF}]]>
	</queryString>
